{
    "explorer.fileNesting.patterns": {
        "*.js": "${capture}.*.js, ${capture}.*.ts, ${capture}.js, ${capture}.js.map",
        "*.ts": "${capture}.*.js, ${capture}.*.ts, ${capture}.js",
        "*.luau": "${capture}.*.lua, ${capture}.*.luau",
        "*.lua": "${capture}.*.lua, ${capture}.*.luau",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": ".luaurc, *.md, *.toml, *.json, *.yml, *.yaml, *.lock",
        "README.md": "*.md",
        "init.cmd": "*.cmd",
        "init.sh": "*.sh",
    },
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": false,
        ".mantle-state.yml": false,
        "wally.lock": false,
        "package-lock.json": false,
        "node_modules": false,
        "sourcemap.json": false,
        "ServerPackages": false,
        "DevPackages": false,
        "Packages": false,
        "out": false,
        ".vscode": false,
        ".github": false,
        ".husky": false
    },
    "yaml.schemas": {
        "https://mantledeploy.vercel.app/schemas/v0.11.15/schema.json": [
            "mantle.yml"
        ],
        "https://json.schemastore.org/github-workflow.json": ".github/workflows/deploy.yml"
    },
    "luau-lsp.types.definitionFiles": [
        "testez.d.luau"
    ],
    "files.eol": "\n",
}